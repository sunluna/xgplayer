{
  "name": "xgplayer",
  "version": "0.0.1",
  "description": "video player",
  "main": "./dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "webpack --progress --display-chunks -p",
    "watch": "webpack --progress --display-chunks -p --watch"
  },
  "keywords": [
    "video",
    "player"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "babel-plugin-bulk-import"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytedance/xgplayer.git"
  },
  "author": "yinguohui@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "node-sass": "^4.8.3"
  },
  "browserslist": [
    "> 5%",
    "IE 9",
    "iOS 7",
    "Firefox > 20"
  ],
  "devDependencies": {
    "event-emitter": "^0.3.5",
    "pasition": "^1.0.1",
    "request-frame": "^1.5.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-bulk-import": "^1.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "core-js": "^2.5.4",
    "css-loader": "^0.28.11",
    "deepmerge": "^1.5.2",
    "draggabilly": "^2.2.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.8.3",
    "node-waf": "^1.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "sugarss": "^1.0.1",
    "webpack": "^4.17.2",
    "webpack-cli": "^2.1.5"
  }
}
